<h1>Lab#create</h1>
<p>Find me in app/views/lab/create.html.erb</p>

<table border="1">
    <tr>
        <td>Номер строки</td>
        <td>Число</td>
    </tr>
        <% if @arr.length > 0 %>
            <% @arr.each_with_index do |el, i| %>
                <tr>
                    <td><%= i + 1 %></td>
                    <td><%= el %></td>
                </tr>
            <% end %>
        <% else %>
            <tr>
                <td colspan="4">Введите другой n!</td>
            </tr>
        <% end %>
</table>